#include <iostream>
using namespace std;
void TOH(int n,char A,char B,char C)
{
    
    if (n == 0) return;

       TOH(n - 1, A, C, B);
       cout << "Move disk " << n << " from " << A << " to " << C << endl;
       TOH(n - 1, B, A, C);
}
int main()
{
    int n;
    cout << "Enter number of disks ";
    cin >> n;

    cout << "Steps to solve Tower of Hanoi for " << n<<endl ;
    TOH(n, 'A', 'B', 'C');

    return 0;
}

    

