// move zero to end
#include <iostream>
using namespace std;
int movzeo(int arr[],int n)
{
    int count=0;
    for(int i=0;i<n;i++)
    {
        if(arr[i]!=0)
        {
            swap(arr[i],arr[count]);
            count++;
        }
    }
    return count ;
    
}
int main()
{
    int arr[]={1,0,2,0,3,0,4,5,0};
    int n=sizeof(arr)/sizeof(arr[0]);
    movzeo(arr,n);
    for(int i=0;i<n;i++)
    {
        cout<<arr[i]<<" ";
    }
}
