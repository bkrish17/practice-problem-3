#include <iostream>
using namespace std;
void printarray(int arr[],int n,int index=0)
{
    if(index==n)
        return ;
    cout<<arr[index]<<" ";
    printarray(arr,n,index+1);
}
int main()
{
    int arr[]={2,4,6,8,10};
    int n=sizeof(arr)/sizeof(arr[0]);
    cout<<"array elements";
    printarray(arr,n);
    return 0;
}
